{
  "extends": "../../tsconfig.base",
  "compilerOptions": {
    "allowJs": false,
    "checkJs": false,
    "jsx": "react-native",
    "lib": ["es2017", "DOM"],
    "types": ["jest", "@testing-library/jest-native/extend-expect"],
    "noEmit": true,
    "target": "esnext",
    "incremental": true,
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "~/*": ["src/*"],
      "LLM@actions/*" : ["./src/actions/*"],
      "LLM@analytics" : ["./src/analytics"],
      "LLM@analytics/*" : ["./src/analytics/*"],
      "LLM@animations/*" : ["./src/animations/*"],
      "LLM@bridge/*" : ["./src/bridge/*"],
      "LLM@components/*" : ["./src/components/*"],
      "LLM@const/*" : ["./src/const/*"],
      "LLM@const" : ["./src/const"],
      "LLM@context/*" : ["./src/context/*"],
      "LLM@dynamicContent/*" : ["./src/dynamicContent/*"],
      "LLM@families/*" : ["./src/families/*"],
      "LLM@families" : ["./src/families"],
      "LLM@helpers/*" : ["./src/helpers/*"],
      "LLM@hooks/*" : ["./src/hooks/*"],
      "LLM@icons/*" : ["./src/icons/*"],
      "LLM@images/*" : ["./src/images/*"],
      "LLM@logic/*" : ["./src/logic/*"],
      "LLM@modals/*" : ["./src/modals/*"],
      "LLM@navigation/*" : ["./src/navigation/*"],
      "LLM@notifications/*" : ["./src/notifications/*"],
      "LLM@reducers/*": ["./src/reducers/*"],
      "LLM@reducers": ["./src/reducers"],
      "LLM@screens/*" : ["./src/screens/*"],
      "LLM@types/*" : ["./src/types/*"],
      "LLM@utils/*" : ["./src/utils/*"],
      "@tests/*": ["src/__tests__/*"]
    }
  },
  "include": ["src/**/*", "e2e/**/*", "**/*.d.ts", "services/**/*"],
  "exclude": ["node_modules", "babel.config.js", "metro.config.js", "jest.config.js", "**/*.js"]
}
